{{#g-maps-container model=model.sites as |map|}}
  {{g-maps lat=map.lat lng=map.lng zoom=map.zoom markers=map.markers}}
{{/g-maps-container}}

{{#if (can 'write company')}}
  {{company-form on-submit=(route-action 'updateCompany') on-delete=(route-action 'deleteCompany') isEditing=true name=model.company.name company=model.company}}
{{/if}}

{{network-sites-list company=model.company sites=model.sites}}
