<h3 class='row-title'>Noovis Distribution Network by Floor</h3>
<div class='row'>
  <div class='col-md-6'>
    <div class='chart-container'>
      {{vertical-bar-chart
        maxBarThickness=maxBarThickness
        selectedSeedColor=selectedSeedColor
        data=data
        maxLabelHeight=maxLabelHeight
        withinGroupPadding=withinGroupPadding
        betweenGroupPadding=betweenGroupPadding
        stackBars=stackBars
        hasXAxisTitle=hasXAxisTitle
        hasYAxisTitle=hasYAxisTitle
        xValueDisplayName=xValueDisplayName
        yValueDisplayName=yValueDisplayName
        xTitleHorizontalOffset=xTitleHorizontalOffset
        yTitleVerticalOffset=yTitleVerticalOffset
      }}
    </div>
  </div>
</div>
